{
  "name": "Баланс абонента Электронного города",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Отображает баланс абонента Электронного города",
  "homepage_url": "2090000.ru",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "default_locale": "ru",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icons/icon19.png",
    "default_title": "browser action demo",
    "default_popup": "src/browser_action/browser_action.html"
  },
  "permissions": [
    "notifications",
    "https://api.novotelecom.ru/billing/*"
  ]
}